  "dependencies": {
    "debug": "^2.6.7",
    "figlet": "^1.2.0",
    "franc": "^3.1.0",
    "hanlp-client": "^1.3.1",
    "koa": "^2.1.0",
    "koa-bodyparser": "^4.2.0",
    "koa-router": "^7.1.1",
    "koa-static": "^3.0.0",
    "lodash": "^4.17.4",
    "node-hanlp-compromise": "^1.0.2",
    "peer": "~0.2.6",
    "socket.io": "~1.0.6",
    "ss-parser": "github:samurais/ss-parser#dist",
    "superagent": "^3.5.2",
